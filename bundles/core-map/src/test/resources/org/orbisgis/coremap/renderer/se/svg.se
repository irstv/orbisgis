<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Style xmlns="http://www.opengis.net/se/2.0/core" xmlns:ns2="http://www.opengis.net/ows/2.0" xmlns:ns3="http://www.opengis.net/fes-2.0" xmlns:ns4="http://www.w3.org/1999/xlink" version="2.0.0">
	<Rule>
		<CompositeSymbolizer>
			<AreaSymbolizer uom="urn:ogc:def:uom:se::mm" version="2.0.0">
				<Extension>
					<ExtensionParameter name="level">1</ExtensionParameter>
				</Extension>

				<Name>Symbolizer Name</Name>
				<SolidFill>
					<Color>#dddddd</Color>
					<Opacity>60</Opacity>
				</SolidFill>
				<PenStroke />
			</AreaSymbolizer>

			<PointSymbolizer uom="urn:ogc:def:uom:se::mm" version="2.0.0">
				<Extension>
					<ExtensionParameter name="level">2</ExtensionParameter>
				</Extension>

				<Name>Default Symbolizer</Name>
				<CompositeGraphic>
					<ExternalGraphic>
						<OnlineResource ns4:href="http://geosysin.iict.ch/house.svg" />
						<Format>image/svg+xml</Format>
						<ViewBox>
							<Width>5.0</Width>
						</ViewBox>
					</ExternalGraphic>

				</CompositeGraphic>
			</PointSymbolizer>
		</CompositeSymbolizer>
	</Rule>
</Style>
